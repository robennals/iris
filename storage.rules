rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /image/{userId}/{allPaths=**} {
      allow read;
      allow write: if request.auth.uid == userId;
    }
    match /thumb/{userId}/{allPaths=**} {
      allow read;
      allow write: if request.auth.uid == userId;
    }
  } 
}
